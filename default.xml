<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <remote fetch="." name="origin" review="https://gitee.com/arkui-x/" />
  <default remote="origin" revision="master" sync-j="4" />

  <include name="openharmony.xml" />
  <project path="build" name="build" revision="arkui-master">
    <linkfile dest="build.sh" src="build_scripts/build.sh"/>
    <linkfile dest=".gn" src="core/gn/dotfile.gn" />
  </project>
  <project path="community" name="community" />
  <project path="samples" name="samples" />
  <project path="xts" name="xts" />
  <project path="docs" name="doc" />
  <project path="developtools/ace_tools" name="cli" />
  <project path="foundation/arkui/ace_engine/adapter/android" name="android" />
  <project path="foundation/arkui/ace_engine/adapter/ios" name="ios" />
  <project path="plugins" name="plugins" />
  <project path="third_party/flutter_ace_shell/engine/flutter" name="third_party_flutter_engine">
    <linkfile dest="third_party/flutter_ace_shell/build" src="arkui/build"/>
    <linkfile dest="third_party/flutter_ace_shell/engine/ace_shell" src="arkui/ace_shell" />
  </project>
</manifest>
