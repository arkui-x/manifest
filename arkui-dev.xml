<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <remote fetch="." name="origin" review="https://gitee.com/arkui-x/" />
  <default remote="origin" revision="arkui-dev" sync-j="4" />

  <include name="openharmony-master.xml" />
  <project path="build" name="build">
    <linkfile dest="build.sh" src="build_scripts/build.sh"/>
    <linkfile dest=".gn" src="core/gn/dotfile.gn" />
  </project>
  <project path="community" name="community" />
  <project path="samples" name="samples" />
  <project path="test/xts" name="xts" />
  <project path="docs" name="docs" />
  <project path="developtools/ace_tools" name="cli" />
  <project path="foundation/arkui/ace_engine/adapter/android" name="arkui_for_android" />
  <project path="foundation/arkui/ace_engine/adapter/ios" name="arkui_for_ios" />
  <project path="plugins" name="plugins" />
</manifest>
